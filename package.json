{
  "name": "multimarketplaces-app",
  "productName": "multimarketplaces-app",
  "version": "0.0.1",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "nodemon --watch src --ext js,ts,vue --exec \"electron-forge start\"",
    "package": "dotenv -e .env electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "npm run package & npm run wdio",
    "wdio": "wdio run ./wdio.conf.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npx ts-node ./scripts/migration-generate.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/main/services/db.ts"
  },
  "build": {
    "files": [
      "dist/**/*",
      "migrations/**/*",
      "src/main/services/**/*"
    ],
    "extraResources": [
      {
        "from": "./src/main/services/db.js",
        "to": "resources/db.js"
      }
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-vite": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@testing-library/webdriverio": "^3.2.1",
    "@types/electron": "^1.4.38",
    "@types/node-fetch": "^2.6.11",
    "@types/steam-tradeoffer-manager": "^2.11.0",
    "@types/steam-user": "^5.0.2",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "@wdio/cli": "^9.3.1",
    "@wdio/local-runner": "^9.3.1",
    "@wdio/mocha-framework": "^9.2.8",
    "@wdio/spec-reporter": "^9.2.14",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^8.0.0",
    "electron": "^32.2.7",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.30.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.4",
    "vite": "^5.4.3",
    "wdio-electron-service": "^7.3.1",
    "wdio-wait-for": "^3.0.11"
  },
  "keywords": [],
  "author": {
    "name": "Ricardo Rocha",
    "email": "ricardocefet2018@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@doctormckay/stdlib": "^2.10.0",
    "@primevue/themes": "^4.0.5",
    "asar": "^3.2.0",
    "async-validator": "^4.2.5",
    "electron-squirrel-startup": "^1.0.1",
    "node-fetch": "^2.7.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.0.5",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "steam-tradeoffer-manager": "^2.11.5",
    "steam-user": "^5.2.0",
    "typeorm": "^0.3.20",
    "vue": "^3.5.3",
    "vue-router": "^4.5.0",
    "ws": "^8.18.0"
  }
}
